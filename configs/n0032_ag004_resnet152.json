{
  "exp_name": "n0032_Ag004_resnet152",
  "out_root_dir": "experiments/",

  "general": {
    "agent": "Ag004_Image",
    "mode": "train",
    "device": "cuda",
    "seed": 42
  },

  "model": {
    "model": "Resnet_000",
    "model_name": "resnet152",
    "type": "fine_tuning"
  },

  "training": {
    "total_epochs": 30
  },

  "dataset": {
    "root_dir":"~/data/built/bonprix/train",
    "perc_val": 0.1,
    "batch_size": 4,
    "num_workers": 8,
    "H": 512,
    "W": 512
  },


  "checkpoint": {
    "checkpoint_filename" : "checkpoint.tar"
  },

  "optimizer": {
    "optim_param": {
      "lr": 0.001,
      "momentum": 0.9
    },
    "factor_decay": 0.3,
    "min_lr": 1e-5,
    "patience": 3
  },

  "losses": {
    "fn" : ["Binary_Cross_Entropy"],
    "weights" : [1]
  }

}
